{
  "name": "@negrel/http-ece",
  "version": "0.7.0",
  "exports": "./mod.ts",
  "compilerOptions": {
    "allowJs": false,
    "strict": true
  },
  "lint": {
    "include": [
      "."
    ],
    "exclude": [
      "dist/",
      "npm/"
    ],
    "rules": {
